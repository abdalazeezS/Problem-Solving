#include <bits/stdc++.h>
using namespace std;
#define int long long
#define debug(x) cout<<"[" << #x << " is:" << x << "] "<<endl;
#define endl '\n'
//By: Tamim


void solve() {
    int l1,r1,l2,r2;
    cin >> l1 >> r1 >> l2 >> r2 ;
    if(l1 > l2){
        swap(l1,l2);
        swap(r1,r2);
    }
    if(l2 >= l1 && l2 <= r1){
        cout << l2 << endl;
    }
    else{
        cout << l1 + l2 << endl;
    }


}

int32_t main() {

    ios::sync_with_stdio(false);cin.tie(0);
    cout.tie(0);
    int t =1 ;
    cin >>t;
    while (t--) {
        solve();
    }



    return 0;
}
